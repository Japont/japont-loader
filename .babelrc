{
  "env": {
    "commonjs": {
      "presets": [
        "es2015-node4",
        "es2016",
        "es2017"
      ],
      "plugins": [
        "add-module-exports"
      ]
    },
    "legacy": {
      "presets": [
        ["env", {
          "targets": {
            "chrome": 49,
            "edge": 13,
            "firefox": 47,
            "safari": 9,
            "ie": 11
          }
        }],
        "es2016",
        "es2017"
      ],
      "plugins": [
        "add-module-exports"
      ]
    },
    "modern": {
      "presets": [
        "modern-browsers",
        "es2017"
      ],
      "plugins": [
        "add-module-exports"
      ]
    },
    "minify": {
      "presets": [
        "babili"
      ],
      "compact": true,
      "minified": false,
      "comments": false
    }
  }
}
