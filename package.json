{
  "name": "japont-loader",
  "version": "0.0.1",
  "author": "3846masa",
  "aliasify": "./aliasify.config.js",
  "browser": "./dist/japont-loader.min.js",
  "browserify-shim": "./browserify-shim.config.js",
  "dependencies": {
    "axios": "^0.15.1",
    "isomorphic-fetch": "^2.2.1",
    "jszip": "^3.1.3",
    "lodash.noop": "^3.0.1"
  },
  "devDependencies": {
    "@types/isomorphic-fetch": "^0.0.31",
    "@types/jszip": "^0.0.31",
    "@types/node": "^6.0.45",
    "babel-cli": "^6.16.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-babili": "^0.0.5",
    "babel-preset-env": "^0.0.6",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-modern-browsers": "^6.0.0",
    "babelify": "^7.3.0",
    "babili": "^0.0.7",
    "browserify": "^13.1.0",
    "browserify-shim": "^3.8.12",
    "cross-env": "^3.1.3",
    "disc": "^1.3.2",
    "eslint": "^3.8.0",
    "eslint-config-airbnb-base": "^9.0.0",
    "eslint-plugin-import": "2",
    "licensify": "^3.1.2",
    "npm-run-all": "^3.1.1",
    "shx": "^0.1.4",
    "typescript": "^2.0.3"
  },
  "files": [
    "LICENSE",
    "README.md",
    "package.json",
    "yarn.lock",
    "esnext",
    "commonjs",
    "dist"
  ],
  "jsnext:main": "esnext/japont-loader.js",
  "keywords": [],
  "license": "Apache-2.0",
  "main": "commonjs/japont.js",
  "scripts": {
    "build": "npm-run-all clean build:init build:esnext -p build:commonjs build:browser",
    "build:browser": "npm-run-all -p build:browser:* -p minify:browser:*",
    "build:browser:legacy": "cross-env BABEL_ENV='legacy' SHIM_ENV='legacy' browserify --standalone JapontLoader -e ./esnext/japont-loader.js -p ./monky-patchify.js -p licensify -t babelify -t browserify-shim -o ./dist/japont-loader.legacy.js",
    "build:browser:main": "cross-env BABEL_ENV='modern' SHIM_ENV='modern' browserify --standalone JapontLoader -e ./esnext/japont-loader.js -p ./monky-patchify.js -p licensify -t babelify -t browserify-shim -o ./dist/japont-loader.js",
    "build:commonjs": "cross-env BABEL_ENV='commonjs' babel ./esnext --out-dir ./commonjs",
    "build:esnext": "tsc",
    "build:init": "shx mkdir dist/ commonjs/ esnext/",
    "clean": "shx rm -r dist/ commonjs/ esnext/",
    "discify": "cross-env BABEL_ENV='legacy' SHIM_ENV='legacy' browserify --standalone JapontLoader -e ./esnext/japont-loader.js -p ./monky-patchify.js -p licensify -t babelify -t browserify-shim --full-paths | discify > disc.html",
    "minify:browser:legacy": "cross-env BABEL_ENV='minify' babel ./dist/japont-loader.legacy.js -o ./dist/japont-loader.legacy.min.js",
    "minify:browser:main": "cross-env BABEL_ENV='minify' babel ./dist/japont-loader.js -o ./dist/japont-loader.min.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  }
}
